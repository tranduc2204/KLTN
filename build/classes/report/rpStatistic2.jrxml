	<queryString>
		<![CDATA[select  TenDichVu, TenNV, TenKH,NgayLapHD,SL, DonGia, (sl* DonGia) as GiaHD  from HoaDonDV hddv join DICHVU dv on hddv.MaDV = dv.MaDV 
join NHANVIEN nv on nv.MaNV = hddv.MaNV join KHACHHANG kh on kh.MaKH = hddv.MaKH
where month(NgayLapHD) = $P{thang} and YEAR (NgayLapHD) = $P{nam}]]>
	</queryString>
