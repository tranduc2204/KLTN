	<queryString>
		<![CDATA[select MaHoaDonPhong, KH.MaKH, HoKH, TenKH, kh.DiaChi, kh.SoDT, kh.GioiTinh, kh.NgaySinh, NgayDatPhong, NgayThuePhong, NgayLapHoaDon, lp.TenLoaiPhong, (DATEDIFF(DAY, NgayThuePhong, NgayLapHoaDon)) as Songay, CONCAT(FORMAT(Tien, 'N0'),' VND') as Tien, CONCAT(FORMAT((((DATEDIFF(DAY, NgayThuePhong, NgayLapHoaDon)) *Tien) * 0.1), 'N0'),' VND') as VAT, CONCAT(FORMAT(((DATEDIFF(DAY, NgayThuePhong, NgayLapHoaDon)) *Tien) + (((DATEDIFF(DAY, NgayThuePhong, NgayLapHoaDon)) *Tien) * 0.1), 'N0'), ' VND')  AS GiaHD
from HoaDonPhong hdp join PhieuThuePhong ptp on hdp.MaPhieuThuePhong = ptp.MaPhieuThuePhong
join PhieuDatPhong pdp on pdp.MaPhieuDatPhong = ptp.MaPhieuDatPhong join KHACHHANG kh on kh.MaKH = pdp.MaKH
join NHANVIEN nv on nv.MaNV = pdp.MaNV join PHONG p on p.MaPhong = pdp.MaPhong join LOAIPHONG lp on lp.MaLoaiPhong = p.MaLoaiPhong]]>
	</queryString>
