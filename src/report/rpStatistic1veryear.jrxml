	<queryString>
		<![CDATA[select MaHoaDonPhong, TenNV, TenKH,TenPhong, NgayLapHoaDon, Tien, CONCAT(FORMAT(Tien, 'N0'),' VND') as Tien1  from HoaDonPhong hdp join PhieuThuePhong ptp 
on hdp.MaPhieuThuePhong = ptp.MaPhieuThuePhong  join PhieuDatPhong pdp on pdp.MaPhieuDatPhong = ptp.MaPhieuDatPhong 
join PHONG p on p.MaPhong = pdp.MaPhong join NHANVIEN nv on nv.MaNV = pdp.MaNV join KHACHHANG kh on kh.MaKH = pdp.MaKH
where YEAR (NgayLapHoaDon) = $P{nam}]]>
	</queryString>
