	<queryString>
		<![CDATA[select MaHoaDonPhong, KH.MaKH, HoKH, TenKH, kh.DiaChi, kh.SoDT,  case when kh.GioiTinh = 0 then 'Nam' when kh.GioiTinh = 1 then N'Nữ' end as GioiTinh
, kh.NgaySinh, NgayDatPhong, NgayDuKienThue, NgayDuKienTra, NgayLapHoaDon, lp.TenLoaiPhong, (DATEDIFF(DAY, NgayDuKienThue, NgayDuKienTra)) as Songay
,CONCAT(FORMAT(DonGia, 'N0'),' VND') as Tien, CONCAT(FORMAT((((DATEDIFF(DAY, NgayDuKienThue, NgayDuKienTra)) *DonGia) * 0.1), 'N0'),' VND') as VAT,
CONCAT(FORMAT((((DATEDIFF(DAY, NgayDuKienThue, NgayDuKienTra)) *DonGia)  +  (((DATEDIFF(DAY, NgayDuKienThue, NgayDuKienTra)) *DonGia) * 0.1) - (((DATEDIFF(DAY, NgayDuKienThue, NgayDuKienTra)) *DonGia) *GiamGia /100)), 'N0'),' VND') AS GiaHD
from HoaDonPhong hdp join PhieuDatPhong pdp on hdp.MaPhieuDatPhong = pdp.MaPhieuDatPhong join phong p on p.MaPhong = pdp.MaPhong
join DonGiaPhong dgp on dgp.MaDonGiaPhong = p.MaDonGiaPhong join LOAIPHONG lp on lp.MaLoaiPhong = p.MaLoaiPhong
join KHACHHANG kh on kh.MaKH = pdp.MaKH 
where MaHoaDonPhong = $P{mahoadonphong}]]>
	</queryString>
